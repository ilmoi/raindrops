{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "lint:check": {
      "outputs": ["^lint:check"]
    },
    "format": {
      "outputs": []
    },
    "format:check": {
      "outputs": []
    },
    "build:lint": {
      "dependsOn": ["lint:check"],
      "outputs": ["dist/**", "build/**", "target/**"]
    },
    "build": {
      "dependsOn": [],
      "outputs": ["dist/**", "build/**", "target/**"]
    },
    "test": {
      "dependsOn": ["build:lint"],
      "outputs": [],
      "inputs": ["tests/**/*.ts"]
    }
  }
}
